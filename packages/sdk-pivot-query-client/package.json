{
  "name": "@sisense/sdk-pivot-query-client",
  "homepage": "https://developer.sisense.com/guides/sdk/",
  "description": "Compose SDK package for interfacing with the pivot backend service in Sisense",
  "repository": {
    "type": "git",
    "url": "https://github.com/sisense/compose-sdk-monorepo",
    "directory": "packages/sdk-pivot-query-client"
  },
  "keywords": [
    "Sisense",
    "Compose SDK"
  ],
  "version": "2.20.0",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Sisense",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc --build tsconfig.build.json",
    "build:prod": "tsc --project tsconfig.prod.json && tsc --project tsconfig.prod.cjs.json && cp package.cjs.json ./dist/cjs/package.json",
    "clean": "rm -rf dist coverage tsconfig.build.tsbuildinfo tsconfig.prod.tsbuildinfo",
    "lint": "echo 'eslint disabled for now.'",
    "lint:fix": "run lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "vitest": "run -T vitest",
    "test": "run vitest run",
    "test:watch": "run vitest watch",
    "test:coverage": "run vitest run --coverage"
  },
  "files": [
    "dist/**/*",
    "esm/**/*"
  ],
  "dependencies": {
    "@sisense/sdk-data": "workspace:*",
    "@sisense/sdk-rest-client": "workspace:*",
    "lodash-es": "^4.17.21",
    "raf": "3.4.0",
    "socket.io-client": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/socket.io-client": "^1.4.36",
    "eslint": "^8.40.0",
    "prettier": "2.8.4",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "socket.io-parser": "3.3.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
