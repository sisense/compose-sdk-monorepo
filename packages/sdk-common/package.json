{
  "name": "@sisense/sdk-common",
  "version": "0.13.0",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Sisense",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "build:prod": "tsc --project tsconfig.prod.json",
    "build.watch": "tsc --build --watch",
    "clean": "rm -rf dist coverage tsconfig.tsbuildinfo tsconfig.prod.tsbuildinfo",
    "format": "run -T prettier --write .",
    "format:check": "run -T prettier --check .",
    "lint": "run -T eslint . --fix",
    "vitest": "run -T vitest",
    "test": "run vitest run",
    "test:watch": "run vitest watch",
    "test:coverage": "run vitest run --coverage"
  },
  "dependencies": {
    "@sisense/sdk-rest-client": "workspace:^",
    "i18next": "23.3.0"
  },
  "devDependencies": {
    "typescript": "4.8.4",
    "vitest": "^0.34.3"
  },
  "packageManager": "yarn@3.2.3",
  "volta": {
    "extends": "../../package.json"
  }
}
