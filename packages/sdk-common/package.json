{
  "name": "@sisense/sdk-common",
  "version": "0.11.0",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/sdk-common.js",
    "default": "./dist/sdk-common.js"
  },
  "main": "./dist/sdk-common.js",
  "module": "./dist/sdk-common.js",
  "types": "./dist/index.d.ts",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Sisense",
  "scripts": {
    "build": "tsc --noEmit && vite build --mode development",
    "build:prod": "tsc --noEmit && vite build",
    "clean": "rm -rf dist coverage tsconfig.tsbuildinfo tsconfig.prod.tsbuildinfo",
    "format": "run -T prettier --write .",
    "format:check": "run -T prettier --check .",
    "lint": "run -T eslint . --fix",
    "vitest": "run -T vitest",
    "test": "run vitest run",
    "test:watch": "run vitest watch",
    "test:coverage": "run vitest run --coverage"
  },
  "dependencies": {
    "@sisense/sdk-rest-client": "workspace:^"
  },
  "devDependencies": {
    "typescript": "4.8.4",
    "vite": "4.1.4",
    "vite-plugin-dts": "^2.3.0"
  },
  "packageManager": "yarn@3.2.3",
  "volta": {
    "extends": "../../package.json"
  }
}
