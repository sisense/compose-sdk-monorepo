{
  "name": "@sisense/sdk-ui-preact",
  "homepage": "https://developer.sisense.com/guides/sdk/",
  "description": "Compose SDK package containing general UI elements and related logic tailored for Preact development",
  "repository": {
    "type": "git",
    "url": "https://github.com/sisense/compose-sdk-monorepo",
    "directory": "packages/sdk-ui-preact"
  },
  "keywords": [
    "Sisense",
    "Compose SDK"
  ],
  "version": "2.21.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ai": {
      "import": "./dist/ai.js",
      "require": "./dist/ai.cjs",
      "default": "./dist/ai.js",
      "types": "./dist/ai.d.ts"
    },
    "./translations/*": {
      "types": "./dist/translations/*.d.ts",
      "require": "./dist/translations/*.cjs",
      "import": "./dist/translations/*.js",
      "default": "./dist/translations/*.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "ai": [
        "./dist/ai.d.ts"
      ],
      "translations/*": [
        "./dist/translations/*.d.ts"
      ]
    }
  },
  "author": "Sisense",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@sisense/sdk-ui": "workspace:*",
    "preact": "^10.26.8",
    "preact-render-to-string": "^6.5.11",
    "react": "^18.3.1 || ^19.0.0",
    "react-dom": "^18.3.1 || ^19.0.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "tsc --noEmit && vite build --mode development",
    "build:prod": "tsc --noEmit && vite build",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "lint": "eslint . --quiet",
    "lint:fix": "run lint --fix",
    "test": "TZ=utc run -T vitest run",
    "test:watch": "TZ=utc run -T vitest watch",
    "test:coverage": "TZ=utc run -T vitest run --coverage",
    "publish:prepare": "node ./scripts/prepare-publish.cjs"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@preact/preset-vite": "2.5.0",
    "@sisense/sdk-data": "workspace:*",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/preact": "^3.2.4",
    "@types/node": "^18.16.0",
    "eslint": "^8.40.0",
    "jsdom": "^22.1.0",
    "prettier": "2.8.4",
    "typescript": "4.8.4",
    "vite": "4.5.2",
    "vite-plugin-dts": "^2.3.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
