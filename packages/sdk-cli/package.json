{
  "name": "@sisense/sdk-cli",
  "description": "CLI for generating programmatic interface from Sisense data model",
  "version": "0.7.3",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Sisense",
  "license": "SEE LICENSE IN LICENSE.md",
  "bin": "./dist/index.js",
  "dependencies": {
    "@sisense/sdk-common": "^0.7.3",
    "@sisense/sdk-data": "^0.7.3",
    "@sisense/sdk-modeling": "^0.7.3",
    "@sisense/sdk-query-client": "^0.7.3",
    "@sisense/sdk-rest-client": "^0.7.3",
    "cross-fetch": "^4.0.0",
    "inquirer": "^8.1.2",
    "yargs": "17.7.1"
  },
  "scripts": {
    "cli": "node ./dist/index.js",
    "prebuild": "node -p \"'export const PKG_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/package-version.ts",
    "build": "run prebuild && tsc --build",
    "build:prod": "run prebuild && tsc --project tsconfig.prod.json",
    "clean": "rm -rf ./node_modules && rm -rf ./dist && rm -rf ./coverage && rm -rf tsconfig.tsbuildinfo",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "files": [
    "src-js/",
    "dist/",
    "./"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/inquirer": "8.2.6",
    "@types/jest": "29.1.2",
    "@types/yargs": "^17.0.22",
    "eslint": "^8.40.0",
    "jest": "29.1.2",
    "prettier": "2.8.4",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
