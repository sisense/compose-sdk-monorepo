{
  "name": "@sisense/sdk-data",
  "version": "0.9.0",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/dimensional-model/filters/filters": "./dist/dimensional-model/filters/filters.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Sisense ",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@sisense/sdk-rest-client": "^0.9.0",
    "guid-typescript": "^1.0.9",
    "numeral": "^2.0.6"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc --build tsconfig.build.json",
    "build:prod": "tsc --project tsconfig.prod.json",
    "build.watch": "tsc --build --watch",
    "clean": "rm -rf dist coverage tsconfig.build.tsbuildinfo tsconfig.prod.tsbuildinfo",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "vitest": "run -T vitest",
    "test": "run vitest run",
    "test:watch": "run vitest watch",
    "test:coverage": "run vitest run --coverage"
  },
  "files": [
    "dist/**/*",
    "esm/**/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/numeral": "2.0.2",
    "eslint": "^8.40.0",
    "prettier": "2.8.4",
    "typescript": "4.8.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
