{
  "name": "@sisense/sdk-ui",
  "homepage": "https://developer.sisense.com/guides/sdk/",
  "description": "Compose SDK package containing general UI elements and related logic tailored for React development",
  "repository": {
    "type": "git",
    "url": "https://github.com/sisense/compose-sdk-monorepo",
    "directory": "packages/sdk-ui"
  },
  "keywords": [
    "Sisense",
    "Compose SDK"
  ],
  "version": "2.18.1",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ai": {
      "types": "./dist/ai.d.ts",
      "require": "./dist/ai.cjs",
      "import": "./dist/ai.js",
      "default": "./dist/ai.js"
    },
    "./analytics-composer": {
      "types": "./dist/analytics-composer.d.ts",
      "require": "./dist/analytics-composer.cjs",
      "import": "./dist/analytics-composer.js",
      "default": "./dist/analytics-composer.js"
    },
    "./analytics-composer/node": {
      "types": "./dist/analytics-composer/node.d.ts",
      "require": "./dist/analytics-composer/node.cjs",
      "import": "./dist/analytics-composer/node.js",
      "default": "./dist/analytics-composer/node.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "ai": [
        "./dist/ai.d.ts"
      ],
      "analytics-composer": [
        "./dist/analytics-composer.d.ts"
      ],
      "analytics-composer/node": [
        "./dist/analytics-composer/node.d.ts"
      ]
    }
  },
  "author": "Sisense",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.11",
    "@mui/material": "^6.4.11",
    "@mui/system": "^6.4.11",
    "@sisense/sdk-common": "workspace:*",
    "@sisense/sdk-data": "workspace:*",
    "@sisense/sdk-pivot-query-client": "workspace:*",
    "@sisense/sdk-pivot-ui": "workspace:*",
    "@sisense/sdk-query-client": "workspace:*",
    "@sisense/sdk-rest-client": "workspace:*",
    "@sisense/sdk-tracking": "workspace:*",
    "@sisense/sisense-charts": "5.2.5",
    "@tanstack/react-query": "4.40.1",
    "classnames": "^2.3.2",
    "colorjs.io": "^0.4.3",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "dayjs": "^1.11.7",
    "deepmerge": "^4.3.1",
    "dompurify": "^3.1.6",
    "fixed-data-table-2": "^2.0.21",
    "geojson": "^0.5.0",
    "guid-typescript": "^1.0.9",
    "highcharts-react-official": "^3.2.1",
    "highcharts-rounded-corners": "^1.0.7",
    "immer": "^10.0.2",
    "leaflet": "^1.9.4",
    "lodash-es": "^4.17.21",
    "markdown-to-jsx": "^7.7.6",
    "proj4leaflet": "^1.0.2",
    "react-datepicker": "^8.4.0",
    "react-error-boundary": "^4.0.11",
    "react-i18next": "^13.2.2",
    "react-number-format": "^5.4.0",
    "superjson": "^2.2.2",
    "ts-deepmerge": "6.0.2",
    "ts-essentials": "^9.3.0",
    "whatwg-fetch": "^3.6.2",
    "yaml": "^2.4.5"
  },
  "scripts": {
    "dev": "vite",
    "type-check": "tsc --noEmit",
    "build": "run type-check && vite build --mode development && run build:umd",
    "build:prod": "run type-check && vite build && run build:umd:prod",
    "build:umd": "tsx ./scripts/vite/vite.umd.build.ts --mode development",
    "build:umd:prod": "tsx ./scripts/vite/vite.umd.build.ts --mode production",
    "clean": "rm -rf ./dist && rm -rf ./coverage && rm -rf tsconfig.tsbuildinfo  && rm -rf node_modules/.vite/deps",
    "lint": "eslint . --quiet",
    "lint:fix": "run lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "vitest": "TZ=utc run -T vitest",
    "test": "run vitest run",
    "test:update": "run vitest -u",
    "test:watch": "run vitest watch",
    "test:coverage": "run vitest run --coverage",
    "test:restructuring": "run vitest run **/*.snapshot.test.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist/**/*",
    "esm/**/*"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@mui/x-data-grid": "^7.29.0",
    "@storybook/addon-docs": "9.1.6",
    "@storybook/addon-links": "9.1.6",
    "@storybook/react-vite": "9.1.6",
    "@storybook/testing-library": "0.2.2",
    "@swc-node/register": "^1.6.5",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7",
    "@types/dompurify": "^3.0.5",
    "@types/fixed-data-table-2": "^1.0.0",
    "@types/leaflet": "^1.9.8",
    "@types/lodash": "^4.14.194",
    "@types/prismjs": "^1.26.5",
    "@types/proj4leaflet": "^1.0.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-plotly.js": "^2.6.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.14",
    "blob-polyfill": "^7.0.20220408",
    "canvas": "^2.11.2",
    "d3": "^7.9.0",
    "eslint": "^8.40.0",
    "jsdom": "^22.1.0",
    "msw": "2.2.1",
    "plotly.js": "^2.25.2",
    "postcss": "^8.4.31",
    "postcss-replace": "^2.0.1",
    "prettier": "2.8.4",
    "prismjs": "^1.30.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-grid-layout": "^1.4.4",
    "react-json-view": "^1.21.3",
    "react-plotly.js": "^2.6.0",
    "rollup-plugin-re": "^1.0.7",
    "sass": "^1.90.0",
    "storybook": "9.1.6",
    "tailwindcss": "^3.3.1",
    "tsx": "^4.19.4",
    "typescript": "4.8.4",
    "vite": "5.4.19",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-css-injected-by-js": "3.0.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest-canvas-mock": "^0.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
