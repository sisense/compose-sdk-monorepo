{
  "name": "@sisense/sdk-ui-vue",
  "homepage": "https://sisense.dev/guides/sdk/",
  "description": "Compose SDK package containing general UI elements and related logic tailored for Vue development",
  "repository": {
    "type": "git",
    "url": "https://github.com/sisense/compose-sdk-monorepo",
    "directory": "packages/sdk-ui-vue"
  },
  "keywords": [
    "Sisense",
    "Compose SDK"
  ],
  "version": "1.33.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ai": {
      "require": "./dist/ai.cjs",
      "import": "./dist/ai.js",
      "types": "./dist/ai.d.ts",
      "default": "./dist/ai.js"
    }
  },
  "typesVersions": {
    "*": {
      "ai": [
        "./dist/ai.d.ts"
      ]
    }
  },
  "author": "Sisense",
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:prod": "run-p type-check build-only:prod",
    "preview": "vite preview",
    "build-only": "vite build --mode development",
    "build-only:prod": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@sisense/sdk-ui-preact": "workspace:^",
    "deepmerge": "^4.3.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.5"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
