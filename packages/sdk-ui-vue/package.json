{
  "name": "@sisense/sdk-ui-vue",
  "homepage": "https://developer.sisense.com/guides/sdk/",
  "description": "Compose SDK package containing general UI elements and related logic tailored for Vue development",
  "repository": {
    "type": "git",
    "url": "https://github.com/sisense/compose-sdk-monorepo",
    "directory": "packages/sdk-ui-vue"
  },
  "keywords": [
    "Sisense",
    "Compose SDK"
  ],
  "version": "2.21.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ai": {
      "require": "./dist/ai.cjs",
      "import": "./dist/ai.js",
      "types": "./dist/ai.d.ts",
      "default": "./dist/ai.js"
    },
    "./translations/*": {
      "types": "./dist/translations/*.d.ts",
      "require": "./dist/translations/*.cjs",
      "import": "./dist/translations/*.js",
      "default": "./dist/translations/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "ai": [
        "./dist/ai.d.ts"
      ],
      "translations/*": [
        "./dist/translations/*.d.ts"
      ]
    }
  },
  "author": "Sisense",
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:prod": "run-p type-check build-only:prod",
    "preview": "vite preview",
    "build-only": "vite build --mode development",
    "build-only:prod": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --quiet",
    "lint:fix": "run lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "vitest": "TZ=utc run -T vitest",
    "test": "run vitest run",
    "test:stop-on-fail": "run vitest run --bail 1",
    "test:update": "run vitest -u",
    "test:watch": "run vitest watch",
    "test:coverage": "run vitest run --coverage",
    "clean": "rm -rf ./dist && rm -rf ./coverage && rm -rf tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@sisense/sdk-ui-preact": "workspace:*",
    "deepmerge": "^4.3.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "~4.8.4",
    "vite": "^4.5.2",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.6.5"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
