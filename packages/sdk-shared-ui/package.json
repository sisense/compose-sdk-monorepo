    "eslint": "^8.40.0",
    "prettier": "3.3.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.83.0",
    "storybook": "^8.4.7",
    "tsx": "^4.19.4",
    "typescript": "4.8.4",
    "vite": "^5.4.11",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4"
  },
